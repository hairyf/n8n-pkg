+						return;
+          const elements = [
+            { selector: '#sidebar', type: 'hidden' },
+            { selector: '.name-container', type: 'hidden' },
+            { selector: '[data-test-id="new-tag-link"]', type: 'visibility:hidden' },
+            { selector: '.action-dropdown-container', type: 'hidden' },
